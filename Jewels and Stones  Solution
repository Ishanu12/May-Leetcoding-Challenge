class Solution {
public:
    int numJewelsInStones(string J, string S) {
        int n1=J.length();
        int n2=S.length();
        int count=0;
        for(int i=0;i<n1;i++)
        {
            for(int j=0;j<n2;j++)
            {
                if(J[i]==S[j])
                {
                    count++;
                }
            }
        }
        return count;
    }
};
